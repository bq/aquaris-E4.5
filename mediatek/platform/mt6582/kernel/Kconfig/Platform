config ARCH_MT6582
        bool "MediaTek MT6582"
        select GENERIC_TIME
        select GENERIC_CLOCKEVENTS
        select ARCH_HAS_CPUFREQ
        select ARM_AMBA
        select CPU_V7
        select HAVE_SMP
        select NEED_MACH_MEMORY_H        
        select ARM_GIC
        select L1C_OPT
        select VFP_OPT
        select HAVE_TRUSTONIC_TEE_SUPPORT
        help
          This enable support for MediaTek MT6582.
