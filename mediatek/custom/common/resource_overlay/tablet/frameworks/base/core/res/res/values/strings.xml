<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <string name="permdesc_disableKeyguard">Allows the app to disable the
      keylock and any associated password security.  For example, the tablet
      disables the keylock when receiving an incoming phone call, then
      re-enables the keylock when the call is finished.</string>
    <string name="wifi_p2p_frequency_conflict_message">The tablet will temporarily disconnect from Wi-Fi while it\'s connected to <xliff:g id="device_name">%1$s</xliff:g></string>
</resources>
